{
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "p", "v"],
      "commands": ["workbench.action.files.openFolder"]
    },
    {
      "before": ["<Esc>"],
      "commands": ["editor.action.clearSearchHighlight"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["editor.action.marker.showQuickFix"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<leader>", "s", "h"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "s", "k"],
      "commands": ["workbench.action.openGlobalKeybindings"]
    },
    {
      "before": ["<leader>", "s", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "s", "w"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    },
    {
      "before": ["<leader>", "s", "g"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "s", "d"],
      "commands": ["workbench.actions.view.problems"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["g", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["g", "r", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["g", "r", "r"],
      "commands": ["references-view.findReferences"]
    },
    {
      "before": ["g", "r", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "r", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "r", "D"],
      "commands": ["editor.action.revealDeclaration"]
    },
    {
      "before": ["g", "O"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["g", "W"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": ["g", "r", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["<Tab>"],
      "commands": ["selectNextSuggestion"]
    },
    {
      "before": ["<S-Tab>"],
      "commands": ["selectPrevSuggestion"]
    },
    {
      "before": ["<C-y>"],
      "commands": ["acceptSelectedSuggestion"]
    },
    {
      "before": ["<C-Space>"],
      "commands": ["editor.action.triggerSuggest"]
    },
    {
      "before": ["<C-e>"],
      "commands": ["hideSuggestWidget"]
    },
    {
      "before": ["<C-d>"],
      "commands": ["showNextParameterHint"]
    },
    {
      "before": ["<C-u>"],
      "commands": ["showPrevParameterHint"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.triggerParameterHints"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    }
  ]
}